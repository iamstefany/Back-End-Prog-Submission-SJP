    {
      "name": "Login as Super Admin",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "23d9037a-f7e9-4dd2-89b0-073b069bf26a",
            "exec": [
              "let jsonData = pm.response.json();\r",
              "\r",
              "if (pm.response.code === 200 && jsonData.accessToken) {\r",
              "    pm.collectionVariables.set(\"JWT_TOKEN\", jsonData.accessToken);\r",
              "} else {\r",
              "    pm.collectionVariables.set(\"JWT_TOKEN\", \"\");\r",
              "}"
